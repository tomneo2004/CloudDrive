# platform :ios, '9.0'

target 'CloudDrive' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'SwiftyDropbox'
  pod 'GoogleAPIClient/Drive', '~> 1.0.2'
  pod 'GTMOAuth2', '~> 1.1.0'
  pod 'GTMAppAuth'

  # Pods for CloudDrive

  target 'CloudDriveTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CloudDriveUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
